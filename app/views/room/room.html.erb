<%= react_component('Room', {
    roomName: @room.slug,
    token: @token,
    roomSid: @twilio_room.sid,
    sessionSlug: @room&.jally_session&.slug,
    roomShared: {
        gameSlug: @room.room_config&.game_slug || @starting_game.slug,
        randomFraction: @room.room_config&.random_fraction || rand(),
        activeParticipant: @active_participant_data,
        roomParticipants: @room.room_participants_data
    }
}) %>